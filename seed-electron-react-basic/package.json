{
  "name": "seed-electron-react-basic",
  "description": "showcases a setup to work with [react] in [electron] without using create-react-app or webpack.",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development electron --require ./script/register.js src/main/index.ts",
    "watch": "yarn compile --watch",
    "compile": "NODE_ENV=production babel src --out-dir build --extensions '.ts,.js,.jsx,.tsx,.svg' --ignore 'src/**/*.spec.ts,src/**/*.spec.js,src/**/*.test.ts,src/**/*.test.js'",
    "build": "yarn compile",
    "test": "yarn jest",
    "prebuild": "yarn clean; yarn fix && yarn lint && yarn checkTypes",
    "checkTypes": "tsc --skipLibCheck false",
    "checkConflicts": "yarn checkTslint && yarn checkEslint",
    "checkTslint": "tslint-config-prettier-check ./tslint.json",
    "checkEslint": "eslint --print-config . | eslint-config-prettier-check",
    "fix": "concurrently \"yarn fix-ts\" \"yarn fix-js\"",
    "lint": "concurrently \"yarn lint-ts\" \"yarn lint-js\"",
    "lint-ts": "tslint 'src/**/*.{ts}'",
    "lint-js": "eslint 'src/**/*.js'",
    "fix-ts": "prettier-tslint fix 'src/**/*.ts'",
    "fix-js": "prettier-eslint 'src/**/*.js' --write",
    "clean": "rm -rf build"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "author": {
    "name": "Juan M. CaÃ±abate (@juanmait)"
  },
  "keywords": [
    "scaffolding",
    "typescript",
    "prettier",
    "jest"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "@types/electron-is-dev": "^0.3.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-react-require": "^3.1.1",
    "babel-preset-react-app": "^7.0.1",
    "concurrently": "^4.1.0",
    "electron": "^4.0.4",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "prettier-eslint-cli": "^4.7.1",
    "prettier-tslint": "^0.4.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "yarn fix-ts",
      "yarn lint-ts",
      "yarn checkTypes"
    ],
    "*.{js}": [
      "yarn fix-js",
      "yarn lint-js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
